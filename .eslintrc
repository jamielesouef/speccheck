{
  "extends": "airbnb-base",
  "rules": {
    "max-len": 0,
    "no-underscore-dangle": 0,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "import/no-extraneous-dependencies": 0,
    "no-multi-str": 0,
    "no-trailing-spaces": 0,
    "no-console": 0,
    "no-unused-vars": 0
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  }
}
